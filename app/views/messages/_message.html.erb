
<li class="<%=  self_or_other(message) %>">
  <div class="avatar">
  	<%=message.user.avatar.present? ? image_tag(message.user.avatar.url ,id: "imgss",class: "img-responsive background") : image_tag('/assets/user.png',class: "img-responsive background")%>
   </div>
  <div class="chatboxmessagecontent">
    <p><%= message.body %></p>
    <time datetime="<%= message.created_at %>" title="<%= message.created_at.strftime("%d %b  %Y at %I:%M%p") %>">
      <%= message_interlocutor(message).fullname %> â€¢ <%= message.created_at.strftime("%H:%M %p") %>
    </time>
  </div>
</li>


