<% if objects_count > 0 %>
  <div id="<%= tree_id %>" style="width:100%; height:100%;"></div>
  <% content_for :page_scripts do %>
    <script>
      $('#<%= tree_id %>').jstree({
        'core' : {
          'data' : <%= data_tree %>
        }
      });
    </script>
  <% end %>
  
<% else %>
  <p class="padding-10"><%= t('no_folders') %></p>
<% end %>


