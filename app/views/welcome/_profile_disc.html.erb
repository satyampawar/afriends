  <div class="row">
	
  <%= form_for @post, url: user_posts_path(current_user.id), html: { multipart: true , remote: true,class: "formData1",:id => 'formData1',:name => 'formData1'}  do |form| %>
						<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
						<div class="col-sm-8">
		<p class="lead emoji-picker-container">					
		<textarea id="post_post_name" class="text_areasize text_my textarea-control form-control" name="post[post_name]"  data-emojiable="true" style="height: 87px;"></textarea>
	</p>

						</div> 

   				        <div class="col-sm-4"style="display: inline-flex;">
   			  				
						  <span class="select-wrapper">
						  <%= form.file_field :photopst, id:"file1" ,:multiple => true, class: "glyphicon glyphicon-camera"%>
						 
						 </span>
						 
						 <%=form.submit "post" ,class: "btn-primary post_btn glyphicon glyphicon-pencil"%>
							
				
      				   </div>
      				   
					<% end %>
              
 </div>
