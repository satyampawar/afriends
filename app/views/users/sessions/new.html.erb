

<div class="container" style="margin-top:30px">
<div class="col-md-6 col-md-offset-1 registration">
    <div class="panel panel-default">
  
  <div class="panel-body">
   

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
   <div class="panel-heading">
            <strong> Sign in to continue</strong>
          </div>
  <div class="alert alert-danger nav_header1">
      <a class="close" data-dismiss="alert" href="#">Ã—</a><%= devise_error_messages! %>
  </div>

    <div class="panel panel-default">
         
          <div class="panel-body">
            <form role="form" action="#" method="POST">
              <fieldset>
                <div class="row">
                  <div class="center-block">
                    <img id="profile-imgs"class="profile-img"
                      src="/assets/user.png " alt="">
                  </div>
                </div>
                <div class="row">
                  <div class="col-sm-12 col-md-10  col-md-offset-1 ">
                    <div class="form-group">
                      <div class="input-group">
                        <span class="input-group-addon">
                          <i class="glyphicon glyphicon-user"></i>
                        </span> 
                        <%= f.email_field :email, autofocus: true ,class: "form-control" ,:placeholder =>"Email"%>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="input-group">
                        <span class="input-group-addon">
                          <i class="glyphicon glyphicon-lock"></i>
                        </span>
                        <%= f.password_field :password, autocomplete: "off" ,class: "form-control",:placeholder =>"password"%>
                      </div>
                    </div>
                    <div class="form-group">
                      <%= f.submit "Sign in" ,class: "btn btn-lg btn-primary btn-block bt-colr"%>
                      
                    </div>
                  </div>
                </div>
              </fieldset>
            </form>
          </div>
           <div class="input-group">
      <div class="checkbox" style="margin-top: 0px;">
        <label>
          <%= f.check_box :remember_me %>Remember me
        </label>

      </div>
    <%end%>  
    </div>
          
          <div class="panel-footer ">
           <p style="color:#008080;">Don't have an account! <%= render "users/shared/links" %></p>
          </div>
                </div>
      </div>
<script>
$(document).ready(function(){

$("#user_email").change(function(){

$email=$("#user_email").val();

 $.ajax
 ({

    method : "get",
    url :  "/photochange?email_id="+$email,
    contentType: false,               
  processData:false, 
    data: "{email:"+$email+ "}",




 })


});
});

</script> 


